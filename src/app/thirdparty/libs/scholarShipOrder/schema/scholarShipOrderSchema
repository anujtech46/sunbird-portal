TableName: scholarShipOrder
Schema: 
  orderId: string
  productId: string
  userId: string
  amount: string
  currency: string
  orderStatus: string <created|userPaidSuccess|userPaidFailed|benefitSuccess|BenefitFailed> 
  createdDate: string
  updatedDate: string

TableName: scholarShipOrderHistory
Schema: 
  orderId: string
  status: string
  note: string
  date: string